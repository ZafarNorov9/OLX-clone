<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/advertisementsFragment">
    <fragment
        android:id="@+id/advertisementsFragment"
        android:name="com.example.olxclone.ui.fragments.AdvertisementsFragment"
        android:label="fragment_advertisements"
        tools:layout="@layout/fragment_advertisements" >
        <action
            android:id="@+id/action_advertisementsFragment_to_buyFragment"
            app:destination="@id/buyFragment" />
    </fragment>
    <fragment
        android:id="@+id/likedFragment"
        android:name="com.example.olxclone.ui.fragments.LikedFragment"
        android:label="fragment_liked"
        tools:layout="@layout/fragment_liked" />
    <fragment
        android:id="@+id/converstaionsFragment"
        android:name="com.example.olxclone.ui.fragments.ConverstaionsFragment"
        android:label="fragment_converstaions"
        tools:layout="@layout/fragment_converstaions" />
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.olxclone.ui.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment2_to_tahrirlash"
            app:destination="@id/tahrirlash" />
    </fragment>
    <fragment
        android:id="@+id/buyFragment"
        android:name="com.example.olxclone.ui.fragments.BuyFragment"
        android:label="fragment_buy"
        tools:layout="@layout/fragment_buy" >
        <action
            android:id="@+id/action_buyFragment_to_advertisementsFragment"
            app:destination="@id/advertisementsFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAdvertise"
        android:name="com.example.olxclone.ui.fragments.FragmentAdvertise"
        android:label="fragment_advertise"
        tools:layout="@layout/fragment_advertise" />
    <fragment
        android:id="@+id/tahrirlash"
        android:name="com.example.olxclone.Tahrirlash"
        android:label="fragment_tahrirlash"
        tools:layout="@layout/fragment_tahrirlash" >
        <action
            android:id="@+id/action_tahrirlash_to_profileFragment2"
            app:destination="@id/profileFragment2" />
    </fragment>
</navigation>